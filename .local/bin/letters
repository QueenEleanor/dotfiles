#!/bin/python
import sys


DIGITS = "0123456789"
ENGLISH_BIG = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"
ENGLISH_SMALL = "abcdefghijklmnopqrstuvwxyz"
SYMBOLS = "!\"#$%&'()*+,-./:;<=>?@[\\]^_`{|}~ "


def main():
    if len(sys.argv) != 2:
        print("usage: letters <options>")
        print("Options:")
        print("    d - 0-9")
        print("    A - A-Z")
        print("    a - a-z")
        print("    s - Other ASCII character")
        sys.exit(-1)

    letters = ""
    if "A" in sys.argv[1]:
        letters += ENGLISH_BIG
    if "a" in sys.argv[1]:
        letters += ENGLISH_SMALL
    if "d" in sys.argv[1]:
        letters += DIGITS
    if "s" in sys.argv[1]:
        letters += SYMBOLS
    
    print(letters)


if __name__ == "__main__":
    main()
